{
  "env": {
    "browser": true,
    "es2021": true
  },
  "root": true,
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id", "__v"] }],
    "no-console": ["error", { "allow": ["warn", "error"] }],
  }
}